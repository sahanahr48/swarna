#include<stdio.h>
int main()
{
    int a[]={10,20,40,50,80};
    int n=sizeof(a)/sizeof(a[0]);
    int index=4;
    int newval=67;
    if(index>=0&&index<n)
    {
        a[3]=newval;
        for(int i=0;i<n;i++)
        printf("%d ",a[i]);
    }
    else
    {
        printf("index out of range");
    }
}
