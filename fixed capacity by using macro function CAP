#include<stdio.h>
#define CAP 30
void printarray(int a[],int n);
void printarray(int a[CAP],int n)
{   printf("["); 
    for(int i=0;i<n&&i<CAP;i++)
    {
        printf("%d",a[i]=i);
    
    }

    
     printf("]");
}
int main()
{
    int a[50];
    printarray(a,45);
    printf("\n");
    return 0;
}


